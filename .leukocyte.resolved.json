{
  "categories": {
    "Layout": {
      "exclude": [
        "./**/bbbb/**/*.rb"
      ],
      "rules": {
        "IndentationConsistency": {
          "enabled": true,
          "enforced_style": "indented_internal_methods",
          "exclude": [
            "zzzz/**/*.rb"
          ]
        },
        "TrailingWhitespace": {
          "enabled": false
        }
      }
    }
  },
  "general": {
    "exclude": [
      "./**/aaaa/**/*.rb"
    ],
    "severity": {
      "warning": "refactor"
    }
  }
}